&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE adm-create-objects D-Dialog _ADM-CREATE-OBJECTS
PROCEDURE adm-create-objects :
/*------------------------------------------------------------------------------
  Purpose:     Create handles for all SmartObjects used in this procedure.
               After SmartObjects are initialized, then SmartLinks are added.
  Parameters:  <none>
------------------------------------------------------------------------------*/
  DEFINE VARIABLE adm-current-page  AS INTEGER NO-UNDO.

  RUN get-attribute IN THIS-PROCEDURE ('Current-Page':U).
  ASSIGN adm-current-page = INTEGER(RETURN-VALUE).

  CASE adm-current-page: 

    WHEN 0 THEN DO:
       RUN init-object IN THIS-PROCEDURE (
             INPUT  'vta/v-guidis.r':U ,
             INPUT  FRAME D-Dialog:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_v-guias ).
       RUN set-position IN h_v-guias ( 1.00 , 1.00 ) NO-ERROR.
       /* Size in UIB:  ( 5.69 , 66.14 ) */

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'adm-vm/objects/p-updv96.r':U ,
             INPUT  FRAME D-Dialog:HANDLE ,
             INPUT  'Edge-Pixels = 2,
                     SmartPanelType = Update,
                     AddFunction = One-Record':U ,
             OUTPUT h_p-updv96 ).
       RUN set-position IN h_p-updv96 ( 6.73 , 17.14 ) NO-ERROR.
       RUN set-size IN h_p-updv96 ( 1.42 , 29.57 ) NO-ERROR.

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'vta/q-guicon.r':U ,
             INPUT  FRAME D-Dialog:HANDLE ,
             INPUT  '':U ,
             OUTPUT h_q-guicon ).
       RUN set-position IN h_q-guicon ( 6.77 , 1.29 ) NO-ERROR.
       /* Size in UIB:  ( 1.35 , 14.86 ) */

       /* Links to SmartViewer h_v-guias. */
       RUN add-link IN adm-broker-hdl ( h_p-updv96 , 'TableIO':U , h_v-guias ).
       RUN add-link IN adm-broker-hdl ( h_q-guicon , 'Record':U , h_v-guias ).

       /* Adjust the tab order of the smart objects. */
       RUN adjust-tab-order IN adm-broker-hdl ( h_v-guias ,
             BUTTON-2:HANDLE , 'BEFORE':U ).
       RUN adjust-tab-order IN adm-broker-hdl ( h_p-updv96 ,
             h_v-guias , 'AFTER':U ).
       RUN adjust-tab-order IN adm-broker-hdl ( h_q-guicon ,
             BUTTON-2:HANDLE , 'AFTER':U ).
    END. /* Page 0 */

  END CASE.

END PROCEDURE.

