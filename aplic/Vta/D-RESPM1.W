 DEFINE VAR W-TOTVEN AS DECIMAL FORMAT "->,>>>,>>9.99" EXTENT 6 INIT 0.
    
 DEFINE FRAME f-cab
        FacCpedm.NroPed FORMAT "XXX-XXXXXX"
        FacCpedm.FchPed 
        Faccpedm.NomCli FORMAT "X(30)"
        Faccpedm.RucCli FORMAT "X(8)"
        Faccpedm.Codven FORMAT "X(4)"
        X-MON           FORMAT "X(4)"
