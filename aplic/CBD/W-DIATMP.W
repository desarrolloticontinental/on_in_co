&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE adm-create-objects W-Win _ADM-CREATE-OBJECTS
PROCEDURE adm-create-objects :
/*------------------------------------------------------------------------------
  Purpose:     Create handles for all SmartObjects used in this procedure.
               After SmartObjects are initialized, then SmartLinks are added.
  Parameters:  <none>
------------------------------------------------------------------------------*/
  DEFINE VARIABLE adm-current-page AS INTEGER NO-UNDO.

  RUN get-attribute IN THIS-PROCEDURE ('Current-Page':U).
  ASSIGN adm-current-page = INTEGER(RETURN-VALUE).

  CASE adm-current-page: 

    WHEN 0 THEN DO:
       RUN init-object IN THIS-PROCEDURE (
             INPUT  'aplic/cbd/v-diatmp.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_v-diatmp ).
       RUN set-position IN h_v-diatmp ( 1.04 , 2.57 ) NO-ERROR.
       /* Size in UIB:  ( 4.19 , 83.86 ) */

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'aplic/cbd/q-diatmp.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  '':U ,
             OUTPUT h_q-diatmp ).
       RUN set-position IN h_q-diatmp ( 5.35 , 2.00 ) NO-ERROR.
       /* Size in UIB:  ( 1.35 , 14.86 ) */

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'aplic/cbd/b-diatmp.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_b-diatmp ).
       RUN set-position IN h_b-diatmp ( 7.04 , 1.43 ) NO-ERROR.
       /* Size in UIB:  ( 8.69 , 84.86 ) */

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'adm/objects/p-navico.r':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Edge-Pixels = 2,
                     SmartPanelType = NAV-ICON,
                     Right-to-Left = First-On-Left':U ,
             OUTPUT h_p-navico ).
       RUN set-position IN h_p-navico ( 5.27 , 11.57 ) NO-ERROR.
       RUN set-size IN h_p-navico ( 1.54 , 21.57 ) NO-ERROR.

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'adm-vm/objects/p-updv05.r':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Edge-Pixels = 2,
                     SmartPanelType = Update,
                     AddFunction = One-Record':U ,
             OUTPUT h_p-updv05 ).
       RUN set-position IN h_p-updv05 ( 5.31 , 34.00 ) NO-ERROR.
       RUN set-size IN h_p-updv05 ( 1.42 , 52.29 ) NO-ERROR.

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'adm-vm/objects/p-updv97.r':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Edge-Pixels = 2,
                     SmartPanelType = Update,
                     AddFunction = One-Record':U ,
             OUTPUT h_p-updv97-2 ).
       RUN set-position IN h_p-updv97-2 ( 13.96 , 47.00 ) NO-ERROR.
       RUN set-size IN h_p-updv97-2 ( 1.54 , 36.57 ) NO-ERROR.

       /* Links to SmartViewer h_v-diatmp. */
       RUN add-link IN adm-broker-hdl ( h_p-updv05 , 'TableIO':U , h_v-diatmp ).
       RUN add-link IN adm-broker-hdl ( h_q-diatmp , 'Record':U , h_v-diatmp ).

       /* Links to SmartQuery h_q-diatmp. */
       RUN add-link IN adm-broker-hdl ( h_p-navico , 'Navigation':U , h_q-diatmp ).

       /* Links to SmartBrowser h_b-diatmp. */
       RUN add-link IN adm-broker-hdl ( h_p-updv97-2 , 'TableIO':U , h_b-diatmp ).
       RUN add-link IN adm-broker-hdl ( h_q-diatmp , 'Record':U , h_b-diatmp ).

    END. /* Page 0 */

  END CASE.

END PROCEDURE.

