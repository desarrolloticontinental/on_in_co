DEF VAR I-NroItm AS INTEGER NO-UNDO.
DEF VAR C-Moneda AS CHAR INIT "S/." FORMAT "X(3)".
DEF VAR F-PRENET AS DECIMAL INIT 0 NO-UNDO.

FIND lg-clistamarco WHERE ROWID(lg-clistamarco) = X-ROWID NO-LOCK NO-ERROR.
IF NOT AVAILABLE lg-clistamarco THEN RETURN. 

IF lg-clistamarco.Codmon = 2 THEN C-Moneda = "US$".

FIND gn-prov WHERE gn-prov.CodCia = PV-CODCIA AND
     gn-prov.CodPro = lg-clistamarco.CodPro NO-LOCK NO-ERROR.

FIND gn-concp WHERE gn-concp.Codig = lg-clistamarco.CndCmp NO-LOCK NO-ERROR.
IF AVAILABLE gn-concp THEN C-ConPgo = gn-concp.Nombr.

