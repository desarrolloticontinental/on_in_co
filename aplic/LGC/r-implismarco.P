PROCEDURE Formato :
/*------------------------------------------------------------------------------
  Purpose:     
  Parameters:  <none>
  Notes:       
------------------------------------------------------------------------------*/
DEFINE FRAME F-HdrLis
    HEADER
    {&PRN2} + {&PRN7A} + {&PRN6A} + S-NOMCIA + {&PRN7B} + {&PRN3} + {&PRN6B} FORMAT "X(45)" 
    {&PRN6A} + " LISTA DE PRECIOS X PROVEEDOR" + {&PRN6B} FORMAT "X(35)" AT 45 
    {&PRN7A} + {&PRN6A} + "LISTA :" + {&PRN6B} + {&PRN7B} + {&PRN3} FORMAT "X(12)" AT 111 
    {&PRN7A} + {&PRN6A} + STRING(lg-clistamarco.NroLis,"999999") + {&PRN6B} + {&PRN7B} + {&PRN3} FORMAT "X(12)" AT 124 SKIP(1)
    "PROVEEDOR : " AT 10 gn-prov.NomPro  AT 25 FORMAT "x(40)"    
    "FECHA EMISION    : " AT 75 lg-clistamarco.FchEmi AT 95 
    "PAG. " AT 126 PAGE-NUMBER(REPORT) FORMAT ">>9" SKIP
    "DIRECCION : " AT 10 gn-prov.DirPro  AT 25 FORMAT "x(40)"    
    "FECHA ACTIVACION : " AT 75 lg-clistamarco.FchVig AT 95 SKIP
    "CONTACTO  : " AT 10 gn-prov.Contactos[1] AT 25   
    "CONDICION COMPRA : " AT 75 C-ConPgo AT 95 SKIP
    WITH PAGE-TOP NO-LABELS NO-BOX STREAM-IO WIDTH 320.

DEFINE FRAME F-DetLis 
       I-NroItm            AT  01 FORMAT "ZZZZ9"        COLUMN-LABEL 'ITEM'
       lg-dlistamarco.Codmat           FORMAT "X(6)"         COLUMN-LABEL 'CODIGO'
       lg-dlistamarco.ArtPro            FORMAT "X(10)"        COLUMN-LABEL 'CODIGO!PROVEEDOR'
       Almmmatg.DesMat            FORMAT "X(40)"        COLUMN-LABEL "D E S C R I P C I O N"
       Almmmatg.DesMar            FORMAT "X(14)"        COLUMN-LABEL "MARCA"
       Almmmatg.UndStk            FORMAT "X(4)"         COLUMN-LABEL "UND"
       C-MONEDA                   FORMAT "X(3)"         COLUMN-LABEL "MON."
       lg-dlistamarco.PreAct           FORMAT ">>,>>9.9999"  COLUMN-LABEL "P.LISTA!ACUAL"
       lg-dlistamarco.Dsctos[1]        FORMAT ">9.99"        COLUMN-LABEL "DSCT1"
       lg-dlistamarco.Dsctos[2]        FORMAT ">9.99"        COLUMN-LABEL "DSCT2"
       lg-dlistamarco.Dsctos[3]        FORMAT ">9.99"        COLUMN-LABEL "DSCT3"
       lg-dlistamarco.IgvMat           FORMAT ">9.99"        COLUMN-LABEL "IGV"
       F-PRENET                   FORMAT ">>>>,>>9.99"  COLUMN-LABEL "COSTO NETO!CON IGV"
       Almmmatg.PreVta[2]                               COLUMN-LABEL "Precio!A"
       Almmmatg.MrgUti-A                                 COLUMN-LABEL "% Margen!A"
       Almmmatg.PreVta[3]                               COLUMN-LABEL "Precio!B"
       Almmmatg.MrgUti-A                                 COLUMN-LABEL "% Margen!B"
       Almmmatg.PreVta[4]                               COLUMN-LABEL "Precio!C"
       Almmmatg.MrgUti-C                                 COLUMN-LABEL "% Margen!C"
       Almmmatg.Dec__01                                 COLUMN-LABEL "Precio!Oficina"
       Almmmatg.PreOfi                                  COLUMN-LABEL "% Margen!Oficina"
    WITH DOWN NO-BOX STREAM-IO WIDTH 320.

DEFINE FRAME F-PagLis
    HEADER
    "                                  -------------------    ---------------------    -------------------" SKIP
    "                                      Operador(a)               Asistente           Jefe de Compras  " SKIP
    "HORA : " AT 1 STRING(TIME,"HH:MM:SS") S-USER-ID AT 40 FORMAT "X(20)" SKIP
    WITH PAGE-BOTTOM NO-LABELS NO-BOX STREAM-IO WIDTH 320.
    
DEFINE FRAME F-SalLis
    HEADER
    " VAN ..." SKIP
    WITH PAGE-BOTTOM NO-LABELS NO-BOX STREAM-IO WIDTH 320.

I-NroItm = 0.
FOR EACH lg-dlistamarco WHERE lg-dlistamarco.CodCia = lg-clistamarco.CodCia AND
    lg-dlistamarco.NroLis = lg-clistamarco.NroLis NO-LOCK,
    FIRST Almmmatg OF lg-dlistamarco NO-LOCK
    BREAK BY lg-dlistamarco.NroLis
    BY lg-dlistamarco.ArtPro
    BY Almmmatg.DesMar
    BY Almmmatg.DesMat : 
    DISPLAY Almmmatg.DesMat @ FI-MENSAJE WITH FRAME F-Proceso.
    VIEW STREAM REPORT FRAME F-HdrLis.
    VIEW STREAM REPORT FRAME F-PagLis.
    I-NroItm = I-NroItm + 1.
    C-MONEDA = IF lg-dlistamarco.Codmon = 1 THEN "S/." ELSE "US$".
    F-PRENET = ROUND (lg-dlistamarco.PreCos + ( lg-dlistamarco.PreCos * (lg-dlistamarco.IgvMat / 100) ),2).
    DISPLAY STREAM REPORT
       I-NroItm
       lg-dlistamarco.ArtPro 
       lg-dlistamarco.Codmat 
       Almmmatg.DesMat
       Almmmatg.DesMar
       Almmmatg.UndStk
       C-MONEDA 
       lg-dlistamarco.PreAct  
       lg-dlistamarco.Dsctos[1] WHEN lg-dlistamarco.Dsctos[1] > 0 
       lg-dlistamarco.Dsctos[2] WHEN lg-dlistamarco.Dsctos[2] > 0 
       lg-dlistamarco.Dsctos[3] WHEN lg-dlistamarco.Dsctos[3] > 0 
       lg-dlistamarco.IgvMat  WHEN lg-dlistamarco.IgvMat > 0
       F-PRENET 
        Almmmatg.PreVta[2]                               
        Almmmatg.MrgUti-A                                 
        Almmmatg.PreVta[3]                               
        Almmmatg.MrgUti-A                                 
        Almmmatg.PreVta[4]                               
        Almmmatg.MrgUti-C                                 
        Almmmatg.Dec__01                                 
        Almmmatg.PreOfi                                  
        WITH FRAME F-DetLis.
    IF LAST-OF(lg-dlistamarco.NroLis) THEN DO:
       VIEW STREAM REPORT FRAME F-PagLis.
       PAGE STREAM REPORT .
    END.
END.

HIDE FRAME F-PROCESO.

END PROCEDURE.

