&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE adm-create-objects W-Win _ADM-CREATE-OBJECTS
PROCEDURE adm-create-objects :
/*------------------------------------------------------------------------------
  Purpose:     Create handles for all SmartObjects used in this procedure.
               After SmartObjects are initialized, then SmartLinks are added.
  Parameters:  <none>
------------------------------------------------------------------------------*/
  DEFINE VARIABLE adm-current-page  AS INTEGER NO-UNDO.

  RUN get-attribute IN THIS-PROCEDURE ('Current-Page':U).
  ASSIGN adm-current-page = INTEGER(RETURN-VALUE).

  CASE adm-current-page: 

    WHEN 0 THEN DO:
       RUN init-object IN THIS-PROCEDURE (
             INPUT  'lgc/b-corcmp.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_b-corcmp ).
       RUN set-position IN h_b-corcmp ( 1.00 , 1.00 ) NO-ERROR.
       /* Size in UIB:  ( 5.54 , 75.00 ) */

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'adm-free/objects/tab95.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'LABEL-FONT = 4,
                     LABEL-FGCOLOR = 0,
                     FOLDER-BGCOLOR = 8,
                     FOLDER-PARENT-BGCOLOR = 8,
                     LABELS = Detalle de O/C|Ingresos Almacen':U ,
             OUTPUT h_tab95 ).
       RUN set-position IN h_tab95 ( 6.58 , 1.00 ) NO-ERROR.
       RUN set-size IN h_tab95 ( 10.58 , 90.00 ) NO-ERROR.

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'adm/objects/p-option.r':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Style = Horizontal Radio-Set,
                     Drawn-in-UIB = yes,
                     Case-Attribute = SortBy-Case,
                     Case-Changed-Event = Open-Query,
                     Dispatch-Open-Query = yes,
                     Edge-Pixels = 2,
                     Label = ':U + '' + ',
                     Link-Name = SortBy-Target,
                     Margin-Pixels = 6,
                     Options-Attribute = SortBy-Options,
                     Font = 4':U ,
             OUTPUT h_p-option ).
       RUN set-position IN h_p-option ( 6.54 , 39.29 ) NO-ERROR.
       RUN set-size IN h_p-option ( 0.92 , 36.86 ) NO-ERROR.

       /* Links to SmartBrowser h_b-corcmp. */
       RUN add-link IN adm-broker-hdl ( h_p-option , 'SortBy':U , h_b-corcmp ).

       /* Links to SmartTab95 h_tab95. */
       RUN add-link IN adm-broker-hdl ( h_tab95 , 'Page':U , THIS-PROCEDURE ).

    END. /* Page 0 */

    WHEN 1 THEN DO:
       RUN init-object IN THIS-PROCEDURE (
             INPUT  'lgc/b-detord.r':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_b-detord ).
       RUN set-position IN h_b-detord ( 7.58 , 1.00 ) NO-ERROR.
       /* Size in UIB:  ( 8.50 , 88.00 ) */

       /* Links to SmartBrowser h_b-detord. */
       RUN add-link IN adm-broker-hdl ( h_b-corcmp , 'Record':U , h_b-detord ).

    END. /* Page 1 */

    WHEN 2 THEN DO:
       RUN init-object IN THIS-PROCEDURE (
             INPUT  'lgc/b-almcmp.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_b-almcmp ).
       RUN set-position IN h_b-almcmp ( 7.58 , 1.43 ) NO-ERROR.
       /* Size in UIB:  ( 3.85 , 74.14 ) */

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'lgc/b-detalm.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_b-detalm ).
       RUN set-position IN h_b-detalm ( 11.46 , 7.29 ) NO-ERROR.
       /* Size in UIB:  ( 5.12 , 62.43 ) */

       /* Links to SmartBrowser h_b-almcmp. */
       RUN add-link IN adm-broker-hdl ( h_b-corcmp , 'Record':U , h_b-almcmp ).

       /* Links to SmartBrowser h_b-detalm. */
       RUN add-link IN adm-broker-hdl ( h_b-almcmp , 'Record':U , h_b-detalm ).

       /* Adjust the tab order of the smart objects. */
    END. /* Page 2 */

  END CASE.
  /* Select a Startup page. */
  IF adm-current-page eq 0 
  THEN RUN select-page IN THIS-PROCEDURE ( 1 ).

END PROCEDURE.

