DEFINE VARIABLE   R AS ROWID.
RUN ADM/H-CTAS01.W( S-CODCIA , 0 , OUTPUT R).
IF R <> ? THEN DO:
   FIND CB-CTAS WHERE ROWID(CB-CTAS) = R
   NO-LOCK NO-ERROR.
   IF AVAIL CB-CTAS THEN SELF:SCREEN-VALUE =
                         CB-CTAS.CODCTA.
   IF {1} THEN {2}:SCREEN-VALUE = CB-CTAS.NOMCTA.                      
END.