/* ************************  Control Triggers  ************************ */

&Scoped-define SELF-NAME BUTTON-Aprobar
&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BUTTON-Aprobar F-Frame-Win
ON CHOOSE OF BUTTON-Aprobar IN FRAME F-Main /* APROBAR */
DO:
    MESSAGE 'Procedemos a APROBAR la lista de precios?'
        VIEW-AS ALERT-BOX QUESTION BUTTONS YES-NO UPDATE rpta AS LOG.
    IF rpta = NO THEN RETURN NO-APPLY.
    RUN Procesa-Botones IN lh_handle ('Aprobar-Mayorista').
END.

/* _UIB-CODE-BLOCK-END */
&ANALYZE-RESUME


&Scoped-define SELF-NAME BUTTON-Rechazar
&ANALYZE-SUSPEND _UIB-CODE-BLOCK _CONTROL BUTTON-Rechazar F-Frame-Win
ON CHOOSE OF BUTTON-Rechazar IN FRAME F-Main /* RECHAZAR */
DO:
    MESSAGE 'Procedemos a RECHAZAR la lista de precios?'
        VIEW-AS ALERT-BOX QUESTION BUTTONS YES-NO UPDATE rpta AS LOG.
    IF rpta = NO THEN RETURN NO-APPLY.
    RUN Procesa-Botones IN lh_handle ('Rechazar-Mayorista').
END.

