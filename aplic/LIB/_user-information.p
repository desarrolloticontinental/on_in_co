/* ***************************  Main Block  *************************** */
DEF INPUT PARAMETER pUserId AS CHAR.
DEF OUTPUT PARAMETER pUserName AS CHAR.
DEF OUTPUT PARAMETER pPassword AS CHAR.
DEF OUTPUT PARAMETER pDisabled AS LOG.

FIND DICTDB._User WHERE DICTDB._User._UserId = pUserId NO-LOCK NO-ERROR.
IF NOT AVAILABLE DICTDB._User THEN RETURN.
ASSIGN
    pUserName = DICTDB._User._User-Name
    pPassword = DICTDB._User._Password
    pDisabled = DICTDB._User._Disabled.
IF pDisabled = ? THEN pDisabled = NO.

