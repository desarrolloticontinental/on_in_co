DEF SHARED VAR s-codcia AS INT.

DEFINE VARIABLE S-CODMOV   AS INTEGER.

FIND FacDocum WHERE FacDocum.CodCia = S-CODCIA AND
    FacDocum.CodDoc = "D/F" NO-LOCK NO-ERROR.
IF AVAILABLE FacDocum THEN S-CODMOV = FacDocum.CodMov.
ELSE DO: 
    MESSAGE 'NO está configurado el movimiento de devolución de mercaderia del cliente' SKIP
        'Revise la configuración de documentos: D/F'
        VIEW-AS ALERT-BOX WARNING.
    RETURN ERROR.
END.

