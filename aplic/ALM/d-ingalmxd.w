&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Carga-Temporal D-Dialog 
PROCEDURE Carga-Temporal :
/*------------------------------------------------------------------------------
  Purpose:     
  Parameters:  <none>
  Notes:       
------------------------------------------------------------------------------*/

EMPTY TEMP-TABLE t-Almacen.

CASE s-user-id:
    WHEN "ADMIN" THEN DO:
        FOR EACH Almacen NO-LOCK WHERE Almacen.CodCia = s-codcia
                AND Almacen.Campo-C[1] = 'XD':
            CREATE T-Almacen.
            BUFFER-COPY Almacen TO T-Almacen.
        END.
    END.
    OTHERWISE DO:
        FOR EACH pf-g006 NO-LOCK WHERE pf-g006.CodCia = s-codcia
            AND pf-g006.Aplic-Id = s-aplic-id
            AND pf-g006.USER-ID = s-user-id,
            FIRST Almacen OF pf-g006 NO-LOCK:
            CREATE T-Almacen.
            BUFFER-COPY Almacen TO T-Almacen.
        END.
    END.
END CASE.

END PROCEDURE.

