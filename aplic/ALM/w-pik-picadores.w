&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE adm-create-objects W-Win  _ADM-CREATE-OBJECTS
PROCEDURE adm-create-objects :
/*------------------------------------------------------------------------------
  Purpose:     Create handles for all SmartObjects used in this procedure.
               After SmartObjects are initialized, then SmartLinks are added.
  Parameters:  <none>
------------------------------------------------------------------------------*/
  DEFINE VARIABLE adm-current-page  AS INTEGER NO-UNDO.

  RUN get-attribute IN THIS-PROCEDURE ('Current-Page':U).
  ASSIGN adm-current-page = INTEGER(RETURN-VALUE).

  CASE adm-current-page: 

    WHEN 0 THEN DO:
       RUN init-object IN THIS-PROCEDURE (
             INPUT  'aplic/alm/b-pik-picador-ocupado.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_b-pik-picador-ocupado ).
       RUN set-position IN h_b-pik-picador-ocupado ( 1.27 , 4.00 ) NO-ERROR.
       RUN set-size IN h_b-pik-picador-ocupado ( 6.69 , 100.00 ) NO-ERROR.

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'aplic/alm/b-pik-picador-ocup-det.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_b-pik-picador-ocup-det ).
       RUN set-position IN h_b-pik-picador-ocup-det ( 8.27 , 4.00 ) NO-ERROR.
       RUN set-size IN h_b-pik-picador-ocup-det ( 6.69 , 100.00 ) NO-ERROR.

       RUN init-object IN THIS-PROCEDURE (
             INPUT  'aplic/alm/b-pik-picador-libre.w':U ,
             INPUT  FRAME F-Main:HANDLE ,
             INPUT  'Layout = ':U ,
             OUTPUT h_b-pik-picador-libre ).
       RUN set-position IN h_b-pik-picador-libre ( 16.08 , 4.00 ) NO-ERROR.
       RUN set-size IN h_b-pik-picador-libre ( 6.69 , 100.00 ) NO-ERROR.

       /* Links to SmartBrowser h_b-pik-picador-ocup-det. */
       RUN add-link IN adm-broker-hdl ( h_b-pik-picador-ocupado , 'Record':U , h_b-pik-picador-ocup-det ).

       /* Adjust the tab order of the smart objects. */
       RUN adjust-tab-order IN adm-broker-hdl ( h_b-pik-picador-ocupado ,
             BUTTON-Reasignar:HANDLE IN FRAME F-Main , 'BEFORE':U ).
       RUN adjust-tab-order IN adm-broker-hdl ( h_b-pik-picador-ocup-det ,
             h_b-pik-picador-ocupado , 'AFTER':U ).
       RUN adjust-tab-order IN adm-broker-hdl ( h_b-pik-picador-libre ,
             BUTTON-Reasignar:HANDLE IN FRAME F-Main , 'AFTER':U ).
    END. /* Page 0 */

  END CASE.

END PROCEDURE.

