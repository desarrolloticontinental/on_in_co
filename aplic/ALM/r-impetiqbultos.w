&ANALYZE-SUSPEND _UIB-CODE-BLOCK _PROCEDURE Imprime-Etiqueta W-Win 
PROCEDURE Imprime-Etiqueta :
/*------------------------------------------------------------------------------
  Purpose:     
  Parameters:  <none>
  Notes:       
------------------------------------------------------------------------------*/

    FIND FIRST almmmatg WHERE almmmatg.codcia = 1
        AND almmmatg.codmat = txt-codigo NO-LOCK NO-ERROR.
    IF AVAIL almmmatg THEN MESSAGE almmmatg.desmat.
    DEF VAR rpta AS LOG.
    SYSTEM-DIALOG PRINTER-SETUP UPDATE rpta.
    IF rpta = NO THEN RETURN.
    OUTPUT STREAM REPORTE TO PRINTER.
        ASSIGN 
            x-codmat = almmmatg.codmat
            x-desmat = almmmatg.desmat
            x-canemp = txt-cantidad.
        CASE rs-cia:
            WHEN "M" THEN x-nomcia = 'METRO'.
            WHEN "T" THEN x-nomcia = 'TOTTUS'.
        END CASE.

        PUT STREAM REPORTE '^XA^LH000,012'               SKIP.   /* Inicia formato */
        {alm/codigoxbultos.i}

        /****
        
        PUT STREAM REPORTE '^LH210,012'                  SKIP.   /* Inicia formato */
        {alm/impetiqinv.i}
      
        PUT STREAM REPORTE '^LH420,012'                  SKIP.   /* Inicia formato */
        {alm/impetiqinv.i}
        
        PUT STREAM REPORTE '^LH630,012'                  SKIP.   /* Inicia formato */
        {alm/impetiqinv.i}
        
        ****/
        
        PUT STREAM REPORTE '^PQ' + TRIM(STRING(1)) SKIP.  /* Cantidad a imprimir */
        PUT STREAM REPORTE '^PR' + '4'                   SKIP.   /* Velocidad de impresion Pulg/seg */
        PUT STREAM REPORTE '^XZ'                         SKIP.   /* Fin de formato */

    OUTPUT STREAM reporte CLOSE.


END PROCEDURE.

