/* BORRAMOS LA INFORMACION DEL ARCHIVO REPLOG */
DEF BUFFER b-replog FOR replog.

PUT UNFORMATTED 'INICIO ' NOW SKIP.
FOR EACH RepLog NO-LOCK WHERE DATE (RepLog.LogDate) < TODAY - 7 :
    FIND b-replog WHERE ROWID(b-replog) = ROWID(replog) EXCLUSIVE-LOCK NO-ERROR NO-WAIT.
    IF AVAILABLE b-replog THEN DELETE b-replog.
END.
PUT UNFORMATTED 'FIN ' NOW SKIP.

QUIT.

